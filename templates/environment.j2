Environment="MB_PASSWORD_COMPLEXITY=normal"
Environment="MB_PASSWORD_LENGTH=10"
Environment="MB_JETTY_HOST={{ metabase_bind }}"
Environment="MB_JETTY_PORT={{ metabase_port }}"
{% if use_own_database %}
Environment="MB_DB_DBNAME={{ metabase_db }}"
Environment="MB_DB_TYPE={{ metabase_db_type }}"
Environment="MB_DB_PORT={{ metabase_db_port }}"
Environment="MB_DB_USER={{ metabase_db_user }}"
Environment="MB_DB_PASS={{ metabase_db_pass }}"
Environment="MB_DB_HOST={{ metabase_db_host }}"
{% endif %}
{% if metabase_use_own_proxy_env %}
Environment="HTTP_PROXY={{ metabase_http_proxy }}"
Environment="HTTPS_PROXY={{ metabase_https_proxy }}"
Environment="NO_PROXY={{ metabase_no_proxy }}"
{% endif %}
Environment="MB_ENCRYPTION_SECRET_KEY={{ secret_metabase_encryption }}"
